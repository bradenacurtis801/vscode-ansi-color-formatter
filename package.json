{
  "name": "ansi-color-formatter",
  "displayName": "ansi-color-formatter",
  "publisher": "bradenacurtis801",
  "description": "",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.98.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bradenacurtis801/vscode-ansi-color-formatter."
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "main": "./ansi-color-formatter/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ansiColor.renderColors",
        "title": "Render ANSI Colors"
      }
    ],
    "languages": [
      {
        "id": "log",
        "extensions": [
          ".log"
        ],
        "aliases": [
          "Log file"
        ],
        "configuration": "./log-language-configuration.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
